<h1>ユーザー登録</h1>
<%= form_with model: @user_form, url: users_path do |form| %>
  <div>
    <%= form.label :name, 'ユーザー名' %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email, 'メールアドレス' %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.submit '登録' %>
  </div>
<% end %>
